{
  "experiment_id": "26ByBiosemi",
  "description": "26ByBiosemi: Citation - Zhao Xiangyue, Zhou Jingyao, Zhang Libo, Duan Haoqing, Wei Shiyu, Bi Yanzhi, and Hu Li (2024). 26 By Biosemi. OpenNeuro. [Dataset] doi: doi:10.18112/openneuro.ds005284.v1.0.0",

  "paths": {
    "raw_dir": "/cifs/seminowicz/eegPainDatasets/CNED/26ByBiosemi",
    "preproc_dir": "/cifs/seminowicz/eegPainDatasets/CNED/da-analysis/26ByBiosemi/Preproc",
    "figures_dir": "/cifs/seminowicz/eegPainDatasets/CNED/da-analysis/26ByBiosemi/Preproc/Figures",
    "logs_dir": "/cifs/seminowicz/eegPainDatasets/CNED/da-analysis/26ByBiosemi/Logs"
  },

  "exp": {
    "id": [001, 002, 003, 004, 005, 006, 007, 008, 009, 010, 011, 012, 013, 014, 015, 016, 017, 018, 019, 020, 021, 022, 023, 024, 025, 026],
    "out_prefix": "26BB_62",
    "raw": {
      "pattern": "sub-%03d_task-26ByBiosemi_eeg.bdf",
      "search_recusiveL": true
    }
  },

  "data": {
    "file_extension": ".set",
    "sampling_rate": 1000,
    "channel_locations": "/cifs/seminowicz/eegPainDatasets/CNED/da-analysis/26ByBiosemi/resources/standard-10-5-cap385.elp",
    "cap_type": "62",

    "event_markers": {
      "pain_stimulus": ["condition 54"],
      "optional_secondary_markers": ""
    }
  },

  "preproc": {
    "line_noise_removal": {
      "apply_notch": false,
      "lowpass_alternative": true,
      "lowpass_cutoff": 40
    },

    "filters": {
      "highpass": 0.5,
      "lowpass": 40,
      "filter_order": 2,
      "apply_fir": true
    },

    "epoching": {
      "apply_epoching": true,
      "event_code": "pain_stimulus",
      "tmin": -1.0,
      "tmax": 2.0,
      "baseline": [-0.5, 0]
    },

    "bad_channel_detection": {
      "auto_detect": true,
      "z-threshold": 4.0,
      "interpolate_before_ica": true
    },

    "reference": {
      "reference_type": "average"
    },

    "ica": {
      "run_ica": true,
      "method": "extended_infomax",
      "reject_by_iclabel": true,
      "iclabel_threshold": 0.8
    }
  },

  "export": {
    "save_cleaned_epochs": true,
    "save_continuous_cleaned": true,
    "output_format": "set"
  }
} 
